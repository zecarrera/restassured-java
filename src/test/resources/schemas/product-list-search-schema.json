{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "products": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "price": {
              "type": "integer",
              "minimum": 0
            },
            "slug": {
              "type": "string"
            }
          },
          "required": ["name", "price", "slug"]
        }
      }
    },
    "required": ["products"]
  }